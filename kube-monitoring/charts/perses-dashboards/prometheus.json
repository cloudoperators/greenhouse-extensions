{
  "kind": "Dashboard",
  "metadata": {
    "name": "prometheus-details",
    "project": "default"
  },
  "spec": {
    "display": {
      "name": "Prometheus Details"
    },
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Prometheus version"
          },
          "plugin": {
            "kind": "TimeSeriesTable",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "count by (version)(prometheus_build_info{job=\"$prometheus\"})",
                    "seriesNameFormat": "{{ version }}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Time since last successful config reload"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "last",
              "format": {
                "unit": "seconds"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "max(time() - prometheus_config_last_reload_success_timestamp_seconds{job=~\"$prometheus\"})",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
          ]
        }
      },
      "2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Last config reload"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "max",
              "thresholds": {
                "steps": [
                  {
                    "color": "#d44a3a",
                    "value": 0
                  },
                  {
                    "color": "rgba(237, 129, 40, 0.89)",
                    "value": 1
                  },
                  {
                    "color": "rgba(50, 172, 45, 0.97)",
                    "value": 0
                  }
                ]
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "max(prometheus_config_last_reload_successful{job=~\"$prometheus\"})",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
          ]
        }
      },
      "3": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "WAL Corruptions"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "max",
              "thresholds": {
                "steps": [
                  {
                    "color": "rgba(50, 172, 45, 0.97)",
                    "value": 0
                  },
                  {
                    "color": "rgba(237, 129, 40, 0.89)",
                    "value": 0.1
                  },
                  {
                    "color": "rgba(245, 54, 54, 0.9)",
                    "value": 1
                  }
                ]
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "max(prometheus_tsdb_wal_corruptions_total{job=~\"$prometheus\"})",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
          ]
        }
      },
      "4": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Memory Usage"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum(container_memory_working_set_bytes{pod=~\"prometheus-$shortPromName-0\", container!=\"\"}) by (namespace, pod, container)",
                    "seriesNameFormat": "{{container}}"
                  }
                }
              }
            }
          ]
        }
      },
      "5": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "CPU usage"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              },
              "yAxis": {
                "format": {
                  "unit": "percent-decimal"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (pod, container) (rate(container_cpu_usage_seconds_total{pod=~\"prometheus-$shortPromName-0\"}[5m]))",
                    "seriesNameFormat": "{{container}}"
                  }
                }
              }
            }
          ]
        }
      },
      "6": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "HTTP server"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              },
              "yAxis": {
                "format": {
                  "unit": "seconds"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "irate(prometheus_http_request_duration_seconds_count{job=~\"$prometheus\"}[2m])",
                    "seriesNameFormat": "{{ handler }}"
                  }
                }
              }
            }
          ]
        }
      },
      "7": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Timeseries"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "prometheus_tsdb_head_series{job=~\"$prometheus\"}",
                    "seriesNameFormat": "{{ job }}"
                  }
                }
              }
            }
          ]
        }
      },
      "8": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Goroutines"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "go_goroutines{job=~\"$prometheus\"}",
                    "seriesNameFormat": "{{  job }}"
                  }
                }
              }
            }
          ]
        }
      },
      "9": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Target Sync"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              },
              "yAxis": {
                "format": {
                  "unit": "seconds"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum(rate(prometheus_target_sync_length_seconds_sum{job=~\"$prometheus\"}[5m])) by (scrape_job) * 1e3",
                    "seriesNameFormat": "{{scrape_job}}"
                  }
                }
              }
            }
          ]
        }
      },
      "10": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Top scrape duration"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "connectNulls": false,
                "stack": "all"
              },
              "yAxis": {
                "format": {
                  "unit": "seconds"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "topk(5, max(scrape_duration_seconds{job=~\"$prometheus\"}) by (job))",
                    "seriesNameFormat": "{{ job }}"
                  }
                }
              }
            }
          ]
        }
      },
      "11": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Query Durations"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "prometheus_engine_query_duration_seconds{job=~\"$prometheus\", quantile=\"0.99\"}",
                    "seriesNameFormat": "{{slice}}_p99"
                  }
                }
              }
            }
          ]
        }
      },
      "12": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Head Chunks"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "prometheus_tsdb_head_chunks{job=~\"$prometheus\"}",
                    "seriesNameFormat": "chunks"
                  }
                }
              }
            }
          ]
        }
      },
      "13": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Reload Count"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "rate(prometheus_tsdb_reloads_total{job=~\"$prometheus\"}[5m])",
                    "seriesNameFormat": "reloads"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "rate(prometheus_tsdb_reloads_failures_total{job=~\"$prometheus\"}[5m])",
                    "seriesNameFormat": "failures"
                  }
                }
              }
            }
          ]
        }
      },
      "14": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Samples Appended"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum(irate(prometheus_tsdb_head_samples_appended_total{job=~\"$prometheus\"}[5m]))",
                    "seriesNameFormat": "samples"
                  }
                }
              }
            }
          ]
        }
      },
      "15": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Active Appenders"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum(prometheus_tsdb_head_active_appenders{job=~\"$prometheus\"})",
                    "seriesNameFormat": "active_appenders"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum(process_open_fds{job=~\"$prometheus\"})",
                    "seriesNameFormat": "open_fds"
                  }
                }
              }
            }
          ]
        }
      },
      "16": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Eval Duration"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "visual": {
                "areaOpacity": 0.3,
                "stack": "all"
              },
              "yAxis": {
                "format": {
                  "unit": "seconds"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "max(prometheus_rule_group_duration_seconds{job=~\"$prometheus\"}) by (quantile)",
                    "seriesNameFormat": "{{quantile}}"
                  }
                }
              }
            }
          ]
        }
      },
      "17": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Eval Activity"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
